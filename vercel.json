{
  "version": 2,
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 0 * * 1-5"
    },
    {
      "path": "/api/cron",
      "schedule": "0 7 * * 1-5"
    },
    {
      "path": "/api/cron",
      "schedule": "0 1 * * 4"
    }
  ]
}
